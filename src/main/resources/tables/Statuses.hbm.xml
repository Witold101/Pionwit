<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class catalog="pionwit_db" name="by.poland.pionwit.db.user.Statuses" optimistic-lock="version" table="statuses">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="name" type="string">
            <column length="250" name="name" not-null="true"/>
        </property>
        <property name="opis" type="string">
            <column length="250" name="opis" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>